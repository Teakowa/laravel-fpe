{
    "name": "teakowa/composer-template",
    "description": "The template for create Composer project",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "template"
    ],
    "authors": [
        {
            "name": "Teakowa",
            "email": "contact@teakowa.dev",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/phpinsights": "^1.11",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Teakowa\\Template\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}